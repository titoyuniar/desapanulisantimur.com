{
  "name": "website-desa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "build:dev": ". ./.env.dev && yarn build",
    "build:prod": ". ./.env.prod && yarn build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier src/**/*.{ts,tsx} --write",
    "prettier": "prettier src/**/*.{ts,tsx} --check",
    "copy:static": "aws s3 sync .next/static s3://erhanesia-webapp/$(echo $STAGE)/kulakin/landing-page/_next/static",
    "copy:public": "aws s3 sync public s3://erhanesia-webapp/$(echo $STAGE)/kulakin/landing-page --delete",
    "s3": "yarn copy:public && yarn copy:static",
    "install:layer": "cd lambda/layer/src/nodejs/node14 && yarn install",
    "predeploy": "yarn build && rm -rf lambda/.next && rm -rf lambda/node_modules && cp -r .next/standalone/.next lambda/.next && cp -r .next/standalone/node_modules lambda/node_modules",
    "predeploy:dev": "rm -rf lambda/.next && rm -rf lambda/node_modules && cp -r .next/standalone/.next lambda/.next && cp -r .next/standalone/node_modules lambda/node_modules",
    "predeploy:prod": "rm -rf lambda/.next && rm -rf lambda/node_modules && cp -r .next/standalone/.next lambda/.next && cp -r .next/standalone/node_modules lambda/node_modules",
    "deploy:soft": "yarn predeploy && sls deploy function -f nextjs && yarn s3",
    "deploy": "sls deploy && yarn s3",
    "deploy:dev": "sls deploy --stage=dev",
    "deploy:prod": "sls deploy --stage=prod"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "5.29.2",
    "@tanstack/react-query-devtools": "5.29.2",
    "axios": "1.6.8",
    "cookies-next": "2.1.1",
    "next": "14.1.0",
    "node-fetch": "^3.3.2",
    "react": "^18",
    "react-dom": "^18",
    "react-indiana-drag-scroll": "^2.2.0",
    "react-query": "3.39.2",
    "react-router-dom": "^6.22.2",
    "react-select": "5.4.0",
    "react-toastify": "9.1.1",
    "recoil": "0.7.6"
  },
  "resolutions": {
    "string-width": "4.2.3",
    "wrap-ansi": "6.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "add": "^2.0.6",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "serverless": "^3.24.1",
    "serverless-http": "^3.1.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.4.2"
  }
}
